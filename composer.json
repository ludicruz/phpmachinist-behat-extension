{
    "name": "derptest/phpmachinist-behat-context",
    "description": "Behat context for managing data objects",
    "license": "MIT",
    "type": "library",
    "keywords": ["machinist", "php", "test", "object", "factory", "blueprint", "behat"],
    "version": "dev-master-yo",
    "authors": [
        {
            "name": "Stephan Soileau",
            "role": "Developer"
        },
        {
            "name": "Adam Englander",
            "homepage": "http://adamknowsstuff.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/derptest/phpmachinst-behat-extension/issues",
        "wiki": "https://github.com/derptest/phpmachinst-behat-extension/wiki",
        "source": "https://github.com/derptest/phpmachinst-behat-extension"
    },
    "autoload": {
        "psr-0": {
            "DerpTest\\Behat\\MachinistExtension": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../phpmachinist/"
        }
    ],
    "require": {
        "php": ">5.3.0",
        "behat/behat": "~3.0",
        "derptest/phpmachinist": "dev-master-yo"
    },
    "require-dev": {
        "phake/phake": "~2.1",
        "phpunit/phpunit": "3.7.*",
        "symfony/expression-language": "^3.2",
        "behat/symfony2-extension": "^2.1"
    }
}
